    />\r\n\r\n      <!-- 查询按钮 -->\r\n      <el-button...></el-button> <!-- 保持原有按钮不变 -->\r\n\r\n        <!-- 时间表展示 -->\r\n        <div v-if=\"schedule\" class=\"timetable\">\r\n          <!-- 表头 -->\r\n          <div class=\"timetable-header\">\r\n            <div class=\"time-header\">时段</div>\r\n            <div\r\n                v-for=\"(day, index) in weekDays\"\r\n                :key=\"index\"\r\n                class=\"day-header\"\r\n            >\r\n              {{ day.date }}<br>{{ day.weekday }}\r\n            </div>\r\n          </div>\r\n\r\n          <!-- 时间行 -->\r\n          <div\r\n              v-for=\"(timeSlot, tIndex) in timeSlots\"\r\n              :key=\"tIndex\"\r\n              class=\"timetable-row\"\r\n          >\r\n            <div class=\"time-label\">{{ timeSlot.label }}<br>{{ timeSlot.range }}</div>\r\n            <div\r\n                v-for=\"(day, dIndex) in weekDays\"\r\n                :key=\"dIndex\"\r\n                class=\"time-cell\"\r\n            >\r\n              <el-button...></el-button> <!-- 保持原有按钮逻辑 -->\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- 确认按钮 -->\r\n        <el-button...></el-button> <!-- 保持原有按钮不变 -->\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      // 保持其他数据不变...\r\n      weekDays: [] // 现在包含周一到周日的7天数据\r\n    }\r\n  },\r\n  methods: {\r\n    handleWeekChange(date) {\r\n      const startDate = new Date(date)\r\n      // 计算本周一的日期（周日为0，周六为6）\r\n      const monday = new Date(startDate)\r\n      monday.setDate(monday.getDate() - (monday.getDay() || 7) + 1)\r\n\r\n      this.weekDays = Array.from({ length: 7 }, (_, i) => {\r\n        const d = new Date(monday)\r\n        d.setDate(d.getDate() + i)\r\n        return {\r\n          date: d.toISOString().split('T'),\r\n          weekday: ['周一','周二','周三','周四','周五','周六','周日'][i]\r\n        }\r\n      })\r\n    },\r\n\r\n    fetchSchedule() {\r\n      // 生成7天的数据\r\n      this.schedule = this.weekDays.reduce((acc, day) => {\r\n        acc[day.date] = this.timeSlots.map(() => Math.random() > 0.6)\r\n        return acc\r\n      }, {})\r\n      this.selectedSlot = null\r\n    },\r\n\r\n    // 保持其他方法不变...\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n/* 新增响应式处理 */\r\n.timetable {\r\n  overflow-x: auto;\r\n}\r\n\r\n.timetable-header,\r\n.timetable-row {\r\n  min-width: 900px; /* 保证7天的最小显示宽度 */\r\n}\r\n\r\n.time-header,\r\n.day-header,\r\n.time-label,\r\n.time-cell {\r\n  min-width: 120px;  /* 原宽度保持不变 */\r\n  flex: 1;\r\n}\r\n\r\n/* 添加周末样式 */\r\n.day-header:nth-child(7),\r\n.day-header:nth-child(7) {\r\n  background: #fef0f0; /* 周六特殊背景 */\r\n}\r\n\r\n.day-header:nth-child(7) {\r\n  background: #fef4ec; /* 周日特殊背景 */\r\n}\r\n\r\n/* 移动端适配 */\r\n@media (max-width: 768px) {\r\n  .day-header br {\r\n    display: none;\r\n  }\r\n\r\n  .day-header {\r\n    font-size: 14px;\r\n  }\r\n\r\n  .time-label {\r\n    font-size: 12px;\r\n  }\r\n\r\n  .slot-btn {\r\n    padding: 5px;\r\n  }\r\n}\r\n</style>\r\n"]}]}